{
  "version": 2,
  "name": "mapv-docs",
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/node",
      "config": {
        "buildCommand": "npm run docs:build",
        "outputDirectory": "docs/.vitepress/dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/docs/.vitepress/dist/$1",
      "status": 200
    }
  ]
}
